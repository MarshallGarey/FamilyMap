<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mgarey2.familymap">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <!-- <uses-amzn-sdk android:minSdkVersion="3" /> -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".person.PersonActivity"
            android:label="@string/title_activity_person">
        </activity>
        <activity android:name=".map.MapActivity">
        </activity>
        <activity android:name=".map.SettingsActivity">
        </activity>
        <activity android:name=".map.FilterActivity">
        </activity>
        <activity android:name=".map.SearchActivity">
        </activity>
    </application>

</manifest>
